{
  "expo": {
    "name": "airflow-calculator",
    "slug": "airflow-calculator",
    "version": "1.5.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "bundleIdentifier": "com.domain.appname",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#FCF8FF",
        "dark":{
          "image": "./assets/images/splash.dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#131318"
        }
      },
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "blockedPermissions": [
        "android.permission.ACTIVITY_RECOGNITION"
      ],
      "package": "com.domain.appname",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#FCF8FF",
        "dark":{
          "image": "./assets/images/splash.dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#131318"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-asset",
        {
          "assets": [
            "assets/markdown/info/de.md",
            "assets/markdown/info/en.md"
          ]
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "organization": "",
          "project": ""
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "xxx-xxx-xxx-xxx"
      }
    },
    "owner": "owner",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/xxx-xxx-xxx-xxx"
    }
  }
}
